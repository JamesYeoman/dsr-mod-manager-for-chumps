{
  "package": {
    "productName": "DSRBMM",
    "version": "0.0.1"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:3000",
    "beforeDevCommand": "yarn dev --clearScreen false",
    "beforeBuildCommand": "yarn build"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "msi",
      "identifier": "io.github.jamesyeoman.dsrbmm",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "Game",
      "shortDescription": "DSRBMM",
      "longDescription": "",
      "windows": {
        "digestAlgorithm": "sha256",
        "certificateThumbprint": null,
        "timestampUrl": "",
        "wix": {
          "license": "../LICENSE"
        }
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": false
    },
    "windows": [
      {
        "title": "Tauri App",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}
